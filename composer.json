{
    "name": "kisphp/elasticsearch",
    "description": "Simple wrapper for php elasticsearch",
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "elasticsearch/elasticsearch": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.6",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "~6.0",
        "symfony/var-dumper": "^3.3"
    },
    "config": {
        "platform": {
            "php": "7.0.8"
        },
        "sort-packages": true
    }
}
